# ElevenLabs MCP Server Configuration
# Copy this file to .env and fill in your values

# Required: Your ElevenLabs API key
# Get it from: https://elevenlabs.io/api-keys
ELEVENLABS_API_KEY=your-api-key-here

# Optional: Logging level
# Options: DEBUG, INFO, WARNING, ERROR
# Default: INFO
LOG_LEVEL=INFO

# Optional: API request timeout in seconds
# Default: 30
API_TIMEOUT=30

# Optional: Cache time-to-live in seconds
# How long to cache API responses
# Default: 300 (5 minutes)
CACHE_TTL=300

# Optional: Maximum retry attempts for failed API calls
# Default: 3
MAX_RETRIES=3

# Optional: ElevenLabs API base URL
# Default: https://api.elevenlabs.io/v1
# Only change if using a proxy or custom endpoint
# API_BASE_URL=https://api.elevenlabs.io/v1

# Optional: Enable debug mode for development
# Default: false
# DEBUG=false

# Optional: Enable advanced features
# Default: false
# ENABLE_ADVANCED_FEATURES=false